{
  "name": "Qzxtu OpenAI Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Generates responses using OpenAI's Chat-GPT / Extension Made by Qzxtu",
  "permissions": ["activeTab", "contextMenus", "clipboardWrite"],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "author": "Qzxtu",
  "homepage_url": "https://github.com/qzxtu"
}
